<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.it_intensive.eli3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.it_intensive.eli3.App">
        <activity android:name=".ActivityStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityMain"></activity>
        <activity
            android:name=".ActivitySearch"
            android:theme="@android:style/Theme.Dialog"
            />
        <activity android:name=".ActivityViewCart"></activity>
        <activity
            android:name=".ActivityChangeIp"
            android:theme="@android:style/Theme.Dialog"
            />
    </application>

</manifest>